{
  "name": "apitte/mapping",
  "description": "Request mappers, transformers and decorators for Apitte stack",
  "keywords": ["api", "apitte", "mapping", "transformers", "decorators"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/apitte/mapping",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 7.2",
    "apitte/core": "^0.1.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "Apitte\\Mapping\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
